[
    {
        "essential": true,
        "image": "${image_url}",
        "name": "${container_name}",
        "environment": [
            {
                "name": "NODE_ENV",
                "value": "development"
            }
        ],
        "portMappings": [
            {
                "containerPort": 80
            }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "${log_group_name}",
                "awslogs-region": "${log_group_region}",
                "awslogs-stream-prefix": "${log_group_prefix}"
            }
        }
    }
]